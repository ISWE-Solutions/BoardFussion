<odoo>
    <template id="template_pdf_viewer" name="Custom PDF Viewer">
        <t t-call="website.layout">
            <t t-set="pdf_url" t-value="pdf_url"/>
            <div class="container">
                <iframe id="pdf_viewer" src="/web/static/src/pdfjs/web/viewer.html?file=${pdf_url}" width="100%" height="800px" style="border: none;"></iframe>
            </div>
        </t>
    </template>
</odoo>
