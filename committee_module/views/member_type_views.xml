<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_member_type_form" model="ir.ui.view">
        <field name="name">Member Type Form</field>
        <field name="model">committee.member.type</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_member_type_tree" model="ir.ui.view">
        <field name="name">committee.member.type</field>
        <field name="model">committee.member.type</field>
        <field name="arch" type="xml">
            <tree string="Member Type">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="action_member_type_list" model="ir.actions.act_window">
        <field name="name">Member Type</field>
        <field name="res_model">committee.member.type</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>